%{count=shops?.size();}%
{
    "status": #{if count>0}0#{/if}#{else}-1#{/else},
    "totalOutlet": ${count},
    "outletList":
    [#{paginate.list1 items:shops, as:'shop'}#{if shop_index > 1 && shop_index % (pageSize+1) !=0},#{/if}{
        "name": "${shop.name}",
        "addr": "${shop.address}",
        "tel": "${shop.phone}",
        "latlng": "${shop?.latitude},${shop?.longitude}"}
    #{/paginate.list1}]
}
