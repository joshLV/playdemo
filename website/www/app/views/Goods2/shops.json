%{count=shops?.size();
page= (pageNumber-1) * pageSize;
}%
{
    "status": #{if count>0}0#{/if}#{else}-1#{/else},
    "totalOutlet": ${count},
    "outletList":
    [#{paginate.list1 items:shops, as:'shop'}#{if shop_index > 1 && (page+1)% shop_index  !=0},#{/if}{
        "name": "${shop.name.escapeJavaScript()}",
        "addr": "${shop.address.escapeJavaScript()}",
        "tel": "${shop.phone.escapeJavaScript()}",
        "latlng": "#{if shop?.latitude && shop?.longitude}${shop?.latitude},${shop?.longitude}#{/if}"}
    #{/paginate.list1}]
}
