<!--获取分类列表 请求 合作伙伴请求-->
<?xml version="1.0" encoding="utf-8"?>
<Request xmlns="http://tuan.360buy.com/Request">
	<!--版本-->
	<Version>1.0</Version>
	<!--合作伙伴ID-->
	<VenderId>100000</VenderId>
	<!--合作伙伴Key-->
	<VenderKey>xxxxxxxxxxxxxxxxx</VenderKey>
	<!--是否压缩 默认不压缩-->
	<Zip>false</Zip>
	<!--是否加密-->
	<Encrypt>true</Encrypt>
	<!--提交数据-->
	<Data>
		<Message xmlns="http://tuan.360buy.com/QueryCategoryRequest">
			<!--一级分类ID 只能为数字 若获取一级分类列表，则此节点不传值或传0，若获取一级分类下二级分类则此节点填写相应一级分类ID 选填-->
			<CategoryId>111</CategoryId>
		</Message>
	</Data>
</Request>