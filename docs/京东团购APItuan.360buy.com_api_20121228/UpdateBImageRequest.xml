<!-- 更新团购主图的接口 请求 合作伙伴请求-->
<?xml version="1.0" encoding="utf-8"?>
<Request xmlns="http://tuan.360buy.com/Request">
	<!--版本-->
	<Version>1.0</Version>
	<!--合作伙伴ID-->
	<VenderId>100000</VenderId>
	<!--合作伙伴Key-->
	<VenderKey>xxxxxxxxxxxxxxxxx</VenderKey>
	<!--是否压缩 默认不压缩-->
	<Zip>false</Zip>
	<!--是否加密-->
	<Encrypt>true</Encrypt>
	<!--提交数据-->
	<Data>
		<Message xmlns="http://tuan.360buy.com/UpdateBImageRequest">
			<!--合作伙伴方团购ID 必须大于0 [必填] -->
			<VenderTeamId>432243</VenderTeamId>
			<!--京东团购ID 必须大于0 [必填] -->
			<JdTeamId>10003943</JdTeamId>
			<!--团购主图  此为图片链接地址http开头，图片尺寸为440*280，图片大小在100kb以内 [必填]-->
			<GrouponBImg>http://images.tuan.360buyimg.com/static/team/2011/1102/13202204309756.jpg</GrouponBImg>
		</Message>
	</Data>
</Request>