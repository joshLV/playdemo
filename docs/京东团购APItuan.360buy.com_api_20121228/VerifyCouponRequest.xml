<!--优惠券验证接口 请求 合作伙伴请求-->
<?xml version="1.0" encoding="utf-8"?>
<Request xmlns="http://tuan.360buy.com/Request">
	<!--版本-->
	<Version>1.0</Version>
	<!--合作伙伴ID-->
	<VenderId>100000</VenderId>
	<!--合作伙伴Key-->
	<VenderKey>xxxxxxxxxxxxxxxxx</VenderKey>
	<!--是否压缩 默认不压缩-->
	<Zip>false</Zip>
	<!--是否加密-->
	<Encrypt>true</Encrypt>
	<!--提交数据-->
	<Data>
		<Message xmlns="http://tuan.360buy.com/VerifyCouponRequest">
			<!--验证类型 0 - 京东券号+京东密码 此模式的选择和上报团购中的CouponVerifyMode节点选择没有关系 [必填]-->
			<VerifyType>0</VerifyType>
			<!--京东订单ID [必填]-->
			<JdOrderId>12232</JdOrderId>
			<!--券号(12位) [必填]-->
			<CouponId>xxxxxxxxxxxxxxxxx</CouponId>
			<!--密码(6位) [必填]-->
			<CouponPwd>xxxxxxxxxxxxxxxxx</CouponPwd>
		</Message>
	</Data>
</Request>