<!--查询优惠券状态的接口 请求 合作伙伴请求-->
<?xml version="1.0" encoding="utf-8"?>
<Request xmlns="http://tuan.360buy.com/Request">
    <!--版本-->
    <Version>1.0</Version>
    <!--合作伙伴ID-->
    <VenderId>100000</VenderId>
    <!--合作伙伴Key-->
    <VenderKey>xxxxxxxxxxxxxxxxxx</VenderKey>
    <!--是否压缩 默认不压缩-->
    <Zip>false</Zip>
    <!--是否加密-->
    <Encrypt>true</Encrypt>
    <!--提交数据-->
    <Data>
		<Message xmlns="http://tuan.360buy.com/QueryCouponRequest">
			<!--京东订单ID [必填]---->
			<JdOrderId>12345</JdOrderId>
			<!--券号12位 [必填]-->
			<CouponId>123456</CouponId>
			<!--密码6位 [必填] -->
			<CouponPwd>234543</CouponPwd>
		</Message>
    </Data>
</Request>