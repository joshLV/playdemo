<!--优惠券密码重置的接口 响应 京东应答-->
<?xml version="1.0" encoding="utf-8"?>
<Response xmlns="http://tuan.360buy.com/Response">
	<!--版本-->
	<Version>1.0</Version>
	<!--合作伙伴ID-->
	<VenderId>100000</VenderId>
	<!--是否压缩 默认不压缩-->
	<Zip>false</Zip>
	<!--是否加密-->
	<Encrypt>true</Encrypt>
	<!--返回结果 (200成功 非200失败)-->
	<ResultCode>200</ResultCode>
	<!--响应信息-->
	<ResultMessage>success</ResultMessage>
	<!--提交数据-->
	<Data>
		<Message xmlns="http://tuan.360buy.com/ResetCouponSecretResponse">
			<!--京东订单ID [必填] ---->
			<JdOrderId>12345</JdOrderId>
			<!--需要重置的合作方优惠券券号 [必填]-->
			<CouponId>xxxxxxxxxxxxxxxxx</CouponId>
			<!--合作方重置后的优惠券密码 [必填]-->
			<CouponPwd>xxxx</CouponPwd>
			<!--重置时间 格式 yyyy-MM-dd HH:mm:ss [必填]-->
			<ResetTime>2012-05-18 10:11:12</ResetTime>
		</Message>
	</Data>
</Response>