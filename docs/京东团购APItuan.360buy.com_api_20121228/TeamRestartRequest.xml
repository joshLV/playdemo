			<VenderTeamId>123456</VenderTeamId>
			<!--团购开始时间 unix时间戳 解析后为 yyyy-MM-dd hh:mm:ss[必填] -->
			<BeginTime>1320595200</BeginTime>
			<!--团购结束时间 unix时间戳 解析后为 yyyy-MM-dd hh:mm:ss[必填] -->
			<EndTime>1320940800</EndTime>
		</Message>
    </Data>
</Request>