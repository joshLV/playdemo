    <!--版本-->
    <Version>1.0</Version>
    <!--合作伙伴ID-->
    <VenderId>100000</VenderId>
    <!--合作伙伴Key-->
    <VenderKey>xxxxxxxxxxxxxxxxxx</VenderKey>
    <!--是否压缩 默认不压缩-->
