<!--更新团购最大库存数接口 应答 京东应答-->
<?xml version="1.0" encoding="utf-8"?>
<Response xmlns="http://tuan.360buy.com/Response">
	<!--版本-->
	<Version>1.0</Version>
	<!--合作伙伴ID-->
	<VenderId>100000</VenderId>
	<!--是否压缩 默认不压缩-->
	<Zip>false</Zip>
	<!--是否加密-->
	<Encrypt>true</Encrypt>
	<!--返回结果(200-成功 300-团购项目不存在 301-团购项目已经下线 500-异常错误)-->
	<ResultCode>200</ResultCode>
	<!--响应信息-->
	<ResultMessage>success</ResultMessage>
	<!--提交数据-->
	<Data>
		<Message xmlns="http://tuan.360buy.com/UpdateMaxNumberResponse">
			<!--合作伙伴方团购ID 必须大于0 [必填] -->
			<VenderTeamId>432243</VenderTeamId>
			<!--京东团购ID 必须大于0 [必填] -->
			<JdTeamId>10003943</JdTeamId>
			<!--团购库存上限 仅数字，必须大于等于0，0表示无上限 [必填]-->
			<MaxNumber>5000</MaxNumber>
		</Message>
	</Data>
</Response>