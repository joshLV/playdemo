<?xml version="1.0" encoding="utf-8"?>
<Request xmlns="http://tuan.jd.com/Request">
    <!-- 查询团购销量接口 请求 京东请求-->
	<!--版本-->
	<Version>1.0</Version>
	<!--合作伙伴ID-->
	<VenderId>100000</VenderId>
	<!--合作伙伴Key-->
	<VenderKey>xxxxxxxxxxxxxxxxx</VenderKey>
	<!--是否压缩 默认不压缩-->
	<Zip>false</Zip>
	<!--是否加密-->
	<Encrypt>false</Encrypt>
	<!--提交数据-->
	<Data>
		<Message xmlns="http://tuan.jd.com/QueryTeamSellCountRequest">
			<!--合作伙伴方团购ID 必须大于0 [必填] -->
			<VenderTeamId>${venderTeamId}</VenderTeamId>
			<!--在京东团购销量（购买份数） 必须大于等于0 [必填] -->
			<SellCount>90</SellCount>
		</Message>
	</Data>
</Request>
